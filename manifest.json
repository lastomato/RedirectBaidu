{
  "manifest_version": 2,

  "name": "Redirect Baidu",
  "description": "This extension redirects requests to baidu.com to cn.bing.com",
  "version": "0.1",

  "background": {
    "scripts": ["run.js"],
    "persistent": true
  },

  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Redirect Baidu"
  },

  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "*://*.baidu.com/s*"
  ]
}